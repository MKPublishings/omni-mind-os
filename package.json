{
  "name": "omni-mind-os",
  "version": "1.0.0",
  "private": true,
  "description": "Omni Ai â€” A Cognitive Operating System",
  "scripts": {
    "dev": "wrangler dev",
    "dev:media": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/dev/start-media-and-worker.ps1",
    "dev:media:local": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/dev/start-media-and-worker.ps1 -Local",
    "deploy": "wrangler deploy",
    "preview": "wrangler dev --local",
    "lint": "npm run lint:code && npm run check:branding",
    "lint:code": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "check:branding": "node ./scripts/check-branding.js",
    "build": "npm run validate:engine",
    "test": "npm run smoke:image && node ./scripts/smoke/maybeVideoSmoke.js",
    "typecheck": "tsc -p tsconfig.json",
    "smoke:image": "node ./omni-image-engine/utils/smokeTest.js",
    "smoke:video": "node ./scripts/smoke/videoRouteSmoke.js",
    "smoke:video:provider": "node ./scripts/smoke/providerVideoSmoke.js",
    "cleanup:video:preview": "node ./scripts/tools/pruneVideoExports.js",
    "cleanup:video": "node ./scripts/tools/pruneVideoExports.js --apply",
    "validate:engine": "node ./omni-image-engine/utils/validator.js",
    "mind:loop": "node ./scripts/mind/runOnRecentLogs.js",
    "mind:auto-debug": "node ./scripts/tools/runAutoDebug.js",
    "mind:tokens": "node ./scripts/tools/printTaskTokens.js",
    "mind:tokens:set": "node ./scripts/tools/updateTaskTokenStatus.js",
    "mind:tokens:active": "node ./scripts/tools/printActiveTaskTokens.js",
    "mind:tokens:next": "node ./scripts/tools/printNextTaskToken.js",
    "mind:tokens:next:claim": "node ./scripts/tools/printNextTaskToken.js --claim",
    "mind:tokens:maintain": "node ./scripts/tools/runTaskTokenMaintenance.js",
    "codex:reindex": "node ./scripts/codex/reindex.js",
    "codex:register": "node ./scripts/codex/registerArtifact.js",
    "codex:watch": "node ./scripts/codex/watch.js"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260227.0",
    "@eslint/js": "^10.0.1",
    "@types/node": "^22.13.4",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.56.1"
  },
  "dependencies": {}
}
